@model List<tbl_Category>

@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered table-striped">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>@Html.ActionLink("Edit", "Edit", "Home", new { Id = item.Id }, htmlAttributes: new { @class= "btn btn-primary" })</td>
        </tr>
    }
</table>



